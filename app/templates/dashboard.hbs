{{partial "navbar"}}

{{ox-map mapReference=mapReference}}

{{#if information}}
<div id="floatingInfo">
    <a type="button" {{action "centerMap" lastLocation}} class="btn btn-embossed btn-primary"><i class="fa fa-car"></i> {{currentCar.name}}</a>
    {{#if speed}}<span><i class="fa fa-tachometer"></i> {{speed}} mph</span>{{/if}}
    {{#if lat}}<span><i class="fa fa-compass"></i> {{lat}} {{lon}}</span>{{/if}}
</div>
{{/if}}